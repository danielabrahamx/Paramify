{
  "version": 1,
  "dfx": "0.29.0",
  "canisters": {
    "paramify_insurance": {
      "type": "rust",
      "package": "paramify_insurance",
      "candid": "icp-canister/src/paramify_insurance.did",
      "declarations": {
        "output": "frontend/src/declarations/paramify_insurance",
        "bindings": ["js", "ts", "did"],
        "node_compatibility": true
      }
    },
    "oracle": {
      "type": "rust",
      "package": "oracle",
      "candid": "src/canisters/oracle/oracle.did",
      "declarations": {
        "output": "frontend/src/declarations/oracle",
        "bindings": ["js", "ts", "did"],
        "node_compatibility": true
      }
    },
    "flood_data_storage": {
      "type": "motoko",
      "main": "icp-integration/src/flood_data_storage/main.mo",
      "declarations": {
        "output": "frontend/src/declarations/flood_data_storage",
        "bindings": ["js", "ts", "did"],
        "node_compatibility": true
      }
    },
    "frontend": {
      "type": "assets",
      "source": ["frontend/dist"],
      "declarations": {
        "output": "frontend/src/declarations/frontend",
        "bindings": ["js", "ts", "did"],
        "node_compatibility": true
      }
    }
  },
  "defaults": {
    "build": {
      "packtool": "",
      "args": ""
    }
  },
  "output_env_file": ".env",
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral",
      "replica": {
        "subnet_type": "system"
      }
    },
    "ic": {
      "providers": ["https://ic0.app"],
      "type": "persistent"
    }
  }
}